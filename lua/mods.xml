<Mods LoadCommand = "%xero(function(self)
	-- judgment / combo proxies
	for pn = 1, 2 do
		setupJudgeProxy(PJ[pn], P[pn]:GetChild('Judgment'), pn)
		setupJudgeProxy(PC[pn], P[pn]:GetChild('Combo'), pn)
	end
	-- player proxies
	for pn = 1, #PP do
		PP[pn]:SetTarget(P[pn])
		P[pn]:x(scx)
		P[pn]:hidden(1)
	end
	-- your code goes here here:
	MESSAGEMAN:Broadcast('Setup')
	function bg(a)
		a:xy(scx, scy)
		a:SetWidth(sw)
		a:SetHeight(sh)
		a:diffuse(0, 0, 0, 1)
	end
	
	bg(HideEvent)

	aft(AFT)
	sprite(AFTSprite)
	aftsprite(AFT, AFTSprite)
	AFTSprite:diffusealpha(0)
	AFT:hidden(1)
	
	-- stop drawing so goddamn much
	lastFrameTime = GAMESTATE:GetSongTime()
	deltaTime = GAMESTATE:GetSongTime() - lastFrameTime
	func {214, 999, function()
    	deltaTime = deltaTime + GAMESTATE:GetSongTime() - lastFrameTime
    	if deltaTime > 0 then
        	AFT:hidden(0)
        	deltaTime = deltaTime - (1 / 60)
    	else
        	AFT:hidden(1)
    	end
    	lastFrameTime = GAMESTATE:GetSongTime()
	end}

	setdefault {3, 'xmod', 100, 'modtimer', 100, 'dizzyholds', 100, 'gayholds', 100, 'halgun'}

	-- i dont like xml
	local songDir = '.' .. GAMESTATE:GetCurrentSong():GetMusicPath() .. '/../'
	loadfile(songDir .. 'lua/sudo.lua')()
	loadfile(songDir .. 'lua/yosefu.lua')()
	
end)"
